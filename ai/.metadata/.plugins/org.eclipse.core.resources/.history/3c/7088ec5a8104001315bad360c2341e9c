package parsing;

public class Lexer {
	
	String[] lexed;
	int position = 0;
	
	public void lexed(String input){
		lexed = input.split(" ");
		position = 0;
	}
	
	public String getNext(){
		if(hasNext()){
			position += 1;
			return lexed[position -1];
		}
		return "";
		
	}
	
	public String peekNext(){
		
	}
	
	public boolean hasNext(){
		return position < lexed.length;
	}	
}
